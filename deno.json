{
  "imports": {
    "@hono/swagger-ui": "npm:@hono/swagger-ui@^0.5.1",
    "@hono/zod-openapi": "npm:@hono/zod-openapi@^0.19.6",
    "@hono/zod-validator": "npm:@hono/zod-validator@^0.5.0",
    "hono": "jsr:@hono/hono@^4.7.9",
    "qrcode": "npm:qrcode@^1.5.4",
    "sharp": "npm:sharp@^0.34.1",
    "zod": "npm:zod@^3.24.4"
  },
  "tasks": {
    "start": "deno run --allow-net --allow-env --allow-read --allow-ffi src/main.ts",
    "dev": "deno run --allow-net --allow-env --allow-read --allow-ffi --watch src/main.ts"
  },
  "nodeModulesDir": "auto",
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "hono/jsx"
  }
}